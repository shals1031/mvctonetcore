<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="description" content="Admin side of TELiCON" />
    <meta name="viewport" content="width=device-width" />
    <noscript>
        <meta http-equiv="refresh" content="0; url=@Url.Action("nojavascript", "error")" />
    </noscript>
    <title>TELiCON | @ViewBag.Title</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,300,700|Montserrat:400,700' rel="stylesheet" type="text/css" />
    <link href="~/jquery.dataTables.css" rel="stylesheet" />
    <link type="text/css" rel="stylesheet" href="~/css/resources/css-resources.css" />
    <link rel="stylesheet" href="http://ajax.googleapis.com/ajax/libs/jqueryui/1.11.2/themes/smoothness/jquery-ui.css" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link href="~/font-awesome.min.css" rel="stylesheet" />
    <link type="text/css" rel="stylesheet" href="~/css/reusables.css" />
    <link type="text/css" rel="stylesheet" href="~/css/admin.css" />

    <script src="~/js/jquery-2.1.1.min.js"></script>

    <style>
        body {
            overflow: auto;
        }
    </style>
    <script>
        $(window).load(function () {
            $(document).on("click", ".print-holder .print-clicker", function (e) {
                window.print();
                e.preventDefault();
            });
        });
    </script>
</head>
<body class="sml @(ViewBag.BodyClass != null ? ViewBag.BodyClass : "")" style="overflow:auto; background-color:#fff; height: 100%;">
    <div style="background:#fff;">
        @RenderBody()
    </div>
    <script src="~/js/jquery-ui.min.js"></script>
    <partial name="_ValidationScriptsPartial" />
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/jquery.globalize/globalize.js"></script>
    <script src="~/js/telicon-bundle.js"></script>
</body>
</html>
